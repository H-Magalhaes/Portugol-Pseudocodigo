programa {
  funcao inicio() {
    inteiro a, b, i, aux

    escreva("Insira primeiro nÃºmero: ")
    leia(a)

    escreva("Insira segundo nÃºmero: ")
    leia(b)

    se(a>b){
      aux=b
      b=a 
      a=aux
    }
    
    escreva("numeros impares\n")
    para(i=a; i<=b; i++){
      se(i%2!=0){
        escreva(i,"\n")
      }
    }

    }
  }
